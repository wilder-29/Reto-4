# Determinar si un numero real es multiplo de otro

def es_multiplo(a, b):
    if b != 0:
        return a % b == 0
    else:
        return False

# Ejemplo:
num1 = float(input("Ingrese el primer número: "))
num2 = float(input("Ingrese el segundo número: "))
print("El primero es múltiplo del segundo:", es_multiplo(num1, num2))
